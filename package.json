{
    "name": "spfx-online-template",
    "version": "0.0.1",
    "private": true,
    "main": "lib/index.js",
    "scripts": {
        "prebuild": "npm run clean && npm run version-sync",
        "preserve": "npm run clean",
        "preship": "npm run clean && npm run version-sync",
        "build": "gulp bundle",
        "clean": "gulp clean && rimraf ./sharepoint && rimraf ./release",
        "trust-dev-cert": "gulp trust-dev-cert",
        "ship": "gulp bundle --ship && gulp package-solution --ship",
        "serve": "gulp bundle && gulp package-solution && gulp serve --no-browser",
        "version-sync": "gulp version-sync"
    },
    "dependencies": {
        "@fluentui/react": "8.77.0",
        "@microsoft/sp-core-library": "1.15.0",
        "@microsoft/sp-lodash-subset": "1.15.0",
        "@microsoft/sp-office-ui-fabric-core": "1.15.0",
        "@microsoft/sp-property-pane": "1.15.0",
        "@microsoft/sp-webpart-base": "1.15.0",
        "@pnp/graph": "^3.4.1",
        "@pnp/sp": "^3.4.1",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recoil": "^0.7.4"
    },
    "devDependencies": {
        "@microsoft/rush-stack-compiler-4.5": "0.2.2",
        "@microsoft/sp-build-web": "1.15.0",
        "@microsoft/sp-module-interfaces": "1.15.0",
        "@types/lodash": "^4.14.182",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/webpack-env": "^1.17.0",
        "gulp": "4.0.2",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "typescript": "4.5.5",
        "yarn": "1.22.19"
    }
}
