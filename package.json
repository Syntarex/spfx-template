{
    "name": "spfx-online-template",
    "version": "0.0.1",
    "private": true,
    "main": "lib/index.js",
    "scripts": {
        "prebuild": "yarn clean && yarn version-sync",
        "preserve": "yarn clean",
        "preship": "yarn clean && yarn lint && yarn version-sync",
        "build": "gulp bundle",
        "clean": "gulp clean && rimraf ./sharepoint && rimraf ./release",
        "lint": "eslint src/**/*.{js,ts,tsx} --no-error-on-unmatched-pattern",
        "trust-dev-cert": "gulp trust-dev-cert",
        "ship": "gulp bundle --ship && gulp package-solution --ship",
        "serve": "gulp bundle && gulp package-solution && gulp serve --no-browser",
        "version-sync": "gulp version-sync"
    },
    "dependencies": {
        "@fluentui/react": "8.105.11",
        "@microsoft/sp-core-library": "1.16.1",
        "@microsoft/sp-lodash-subset": "1.16.1",
        "@microsoft/sp-office-ui-fabric-core": "1.16.1",
        "@microsoft/sp-property-pane": "1.16.1",
        "@microsoft/sp-webpart-base": "1.16.1",
        "@pnp/graph": "^3.12.1",
        "@pnp/sp": "^3.12.1",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recoil": "^0.7.6"
    },
    "devDependencies": {
        "@microsoft/rush-stack-compiler-4.5": "0.4.0",
        "@microsoft/sp-build-web": "1.16.1",
        "@microsoft/sp-module-interfaces": "1.16.1",
        "@types/lodash": "^4.14.191",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/webpack-env": "^1.18.0",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "gulp": "4.0.2",
        "prettier": "^2.8.4",
        "rimraf": "^4.1.2",
        "typescript": "4.5.5",
        "yarn": "1.22.19"
    }
}
