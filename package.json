{
    "name": "spfx-online-template",
    "version": "0.0.1",
    "private": true,
    "main": "lib/index.js",
    "scripts": {
        "prebuild": "yarn clean && yarn version-sync",
        "preserve": "yarn clean && yarn version-sync",
        "preship": "yarn clean && yarn lint && yarn version-sync",
        "build": "gulp bundle",
        "clean": "gulp clean && rimraf ./sharepoint && rimraf ./release",
        "lint": "eslint src/**/*.{js,ts,tsx} --no-error-on-unmatched-pattern",
        "trust-dev-cert": "gulp trust-dev-cert",
        "ship": "gulp bundle --ship && gulp package-solution --ship",
        "serve": "gulp bundle && gulp package-solution && gulp serve --no-browser",
        "version-sync": "gulp version-sync"
    },
    "dependencies": {
        "@fluentui/react": "8.112.9",
        "@microsoft/sp-core-library": "1.18.2",
        "@microsoft/sp-property-pane": "1.18.2",
        "@microsoft/sp-webpart-base": "1.18.2",
        "@pnp/graph": "3.20.1",
        "@pnp/sp": "3.20.1",
        "lodash": "4.17.21",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "recoil": "0.7.7"
    },
    "devDependencies": {
        "@microsoft/rush-stack-compiler-4.7": "0.1.0",
        "@microsoft/sp-build-web": "1.18.2",
        "@microsoft/sp-module-interfaces": "1.18.2",
        "@types/lodash": "4.14.202",
        "@types/react": "18.2.42",
        "@types/react-dom": "18.2.17",
        "@types/webpack-env": "1.18.4",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.2",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "gulp": "4.0.2",
        "prettier": "3.1.0",
        "rimraf": "5.0.5",
        "typescript": "4.7.4",
        "yarn": "1.22.21"
    }
}
